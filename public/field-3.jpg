<svg width="600" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grassGradient3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#84cc16;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#65a30d;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#4d7c0f;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#365314;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
    <pattern id="grassTexture" patternUnits="userSpaceOnUse" width="15" height="15">
      <rect width="15" height="15" fill="url(#grassGradient3)"/>
      <rect width="7" height="7" fill="#65a30d" opacity="0.4"/>
      <rect x="8" y="8" width="7" height="7" fill="#4d7c0f" opacity="0.3"/>
    </pattern>
  </defs>
  
  <!-- Sky with clouds -->
  <rect width="600" height="140" fill="url(#skyGradient)"/>
  <ellipse cx="150" cy="60" rx="40" ry="20" fill="white" opacity="0.8"/>
  <ellipse cx="450" cy="40" rx="60" ry="25" fill="white" opacity="0.7"/>
  <ellipse cx="350" cy="80" rx="35" ry="15" fill="white" opacity="0.6"/>
  
  <!-- Field background -->
  <rect y="140" width="600" height="260" fill="url(#grassTexture)"/>
  
  <!-- Field outline -->
  <rect x="70" y="170" width="460" height="230" fill="none" stroke="white" stroke-width="4"/>
  
  <!-- Center line -->
  <line x1="300" y1="170" x2="300" y2="400" stroke="white" stroke-width="3"/>
  
  <!-- Center circle -->
  <circle cx="300" cy="285" r="45" fill="none" stroke="white" stroke-width="3"/>
  <circle cx="300" cy="285" r="2" fill="white"/>
  
  <!-- Goals -->
  <rect x="70" y="240" width="20" height="90" fill="none" stroke="white" stroke-width="3"/>
  <rect x="510" y="240" width="20" height="90" fill="none" stroke="white" stroke-width="3"/>
  
  <!-- Goal areas -->
  <rect x="70" y="220" width="60" height="130" fill="none" stroke="white" stroke-width="2"/>
  <rect x="470" y="220" width="60" height="130" fill="none" stroke="white" stroke-width="2"/>
  
  <!-- Trees in background -->
  <ellipse cx="30" cy="120" rx="15" ry="30" fill="#166534"/>
  <ellipse cx="570" cy="110" rx="20" ry="35" fill="#166534"/>
  <ellipse cx="580" cy="130" rx="12" ry="25" fill="#15803d"/>
  
  <!-- Stadium lights -->
  <rect x="80" y="90" width="6" height="80" fill="#374151"/>
  <circle cx="83" cy="85" r="6" fill="#fbbf24"/>
  <rect x="520" y="90" width="6" height="80" fill="#374151"/>
  <circle cx="523" cy="85" r="6" fill="#fbbf24"/>
  
  <!-- Natural grass stripes -->
  <rect x="70" y="170" width="460" height="15" fill="#65a30d" opacity="0.3"/>
  <rect x="70" y="200" width="460" height="15" fill="#65a30d" opacity="0.3"/>
  <rect x="70" y="230" width="460" height="15" fill="#65a30d" opacity="0.3"/>
  <rect x="70" y="260" width="460" height="15" fill="#65a30d" opacity="0.3"/>
  <rect x="70" y="290" width="460" height="15" fill="#65a30d" opacity="0.3"/>
  <rect x="70" y="320" width="460" height="15" fill="#65a30d" opacity="0.3"/>
  <rect x="70" y="350" width="460" height="15" fill="#65a30d" opacity="0.3"/>
  <rect x="70" y="380" width="460" height="15" fill="#65a30d" opacity="0.3"/>
</svg> 